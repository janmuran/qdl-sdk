{
  "name": "janmuran/qdl-sdk",
  "description": "QDL php client",
  "keywords": [
    "qdl",
    "qdl api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/janmuran/qdl-sdk",
  "authors": [
    {
      "name": "Jan Muran",
      "email": "muran@muran.at",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "mockery/mockery": "^1.4",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.10"
  },
  "autoload": {
    "psr-4": {
      "Janmuran\\QdlSdk\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Janmuran\\QdlSdk\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true
    }
  }
}
